{
  "name": "@zoolabs/uniswap-v2-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/zoo-labs/uni-v2-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "run-s codegen && graph build",
    "buildonly": "graph build",
    "codegen": "graph codegen --output-dir src/types/",
    "create-zoo": "graph create zoo/uniswap-v2 --node https://graph.zoo.network",
    "deploy-zoo": "RUST_LOG=debug graph deploy zoo/uniswap-v2 --ipfs https://ipfs.lux.network --node https://${GRAPH_ADMIN_USER}:${GRAPH_ADMIN_PASS}@graph-admin.zoo.network",
    "watch-local": "graph deploy zoo/uniswap-v2 --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.64.1",
    "@graphprotocol/graph-ts": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@uniswap/eslint-config": "^1.2.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
